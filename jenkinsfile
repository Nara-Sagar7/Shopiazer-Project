node   {
    stage('vcs') {
       git 'https://github.com/Nara-Sagar7/Shopiazer-Project.git'
    }
   
    stage('build') {
       sh'mvn package'
    }   
   
    stage('archive') {
       archive 'target/*.jar'
    }
   
    stage('junit') {
       junit 'target/surefire-reports/*.xml'
    }
   
}